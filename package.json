{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "start": "nuxt start",
    "build": "npx nuxi typecheck && nuxt build",
    "dev": "npx nuxi typecheck && nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
