    "@stripe/stripe-js": "^3.0.6",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@typescript-eslint/typescript-estree": "^7.10.0",
    "next-auth": "~4.21.1",
    "node-fetch": "^2.7.0",
    "nuxt": "^3.10.3",
    "pinia": "^2.1.7",
    "postcss-custom-media": "^10.0.2",
